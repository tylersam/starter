<div class="navbar navbar-blue">


        <div class="navbar-header">
          <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-inverse-collapse">
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
          </button>
          <%= link_to "HAWKEYE2", root_path, class: "navbar-brand" %>
        </div>
        <div class="navbar-collapse collapse navbar-inverse-collapse">
          <form class="navbar-form navbar-nav form-horizontal">
            <div class="form-group">
              <select class="form-control" id="unit_select">
                <option>All Units</option>
                <option>Oak St</option>
                <option>Dave Ward</option>
              </select>
            </div>
          </form>
          <ul class="nav navbar-nav">
            <li><a href="javascript:void(0)">Sales</a></li>
            <li><a href="javascript:void(0)">Labor</a></li>
            <li class="dropdown">
              <a href="index.html" data-target="#" class="dropdown-toggle" data-toggle="dropdown">Meal4Meal <b class="caret"></b></a>
              <ul class="dropdown-menu">
                <li><a href="javascript:void(0)">Meals</a></li>
                <li class="divider"></li>
                <li><a href="javascript:void(0)">Tip Jar</a></li>
                <li><a href="javascript:void(0)">One Time Donation</a></li>
              </ul>
            </li>
          </ul>

          <ul class="nav navbar-nav navbar-right">
            <li class="dropdown">
              <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-expanded="false">Admin <span class="caret"></span></a>
              <ul class="dropdown-menu" role="menu">
                <li><a href="#">Users</a></li>
                <li><%= link_to "Approved Users", approved_users_path %></li>
              </ul>
            </li>
            <% if user_signed_in? %>
                <li class="link_style">Logged in as <strong><%= current_user.first_name %></strong></li>
                <li><%= link_to "Logout", destroy_user_session_path, method: :delete %></li>
            <% else %>
                <li><%= link_to "Sign in", user_omniauth_authorize_path(:google_oauth2) %></li>
            <% end %>

          </ul>
        </div>

  </div>





